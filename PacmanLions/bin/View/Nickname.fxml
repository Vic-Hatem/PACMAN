<AnchorPane prefHeight="546.0" prefWidth="637.0" style="-fx-background-color: black;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="View.NicknameController">
