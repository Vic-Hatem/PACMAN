<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.mainPageControl">
   <top>
      <ImageView fitHeight="142.0" fitWidth="489.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <image>
            <Image url="@../pacmanTitle.PNG" />
         </image>
      </ImageView>
   </top>
   <left>
      <ImageView fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <image>
            <Image url="@../PacmanLogo.PNG" />
         </image>
      </ImageView>
   </left>
   <right>
      <VBox alignment="CENTER" prefHeight="482.0" prefWidth="247.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="startGame" blendMode="SCREEN" mnemonicParsing="false" onAction="#StartGame" opacity="0.94" prefHeight="51.0" prefWidth="198.0" style="-fx-background-color: #0000CD;" text="START GAME" textAlignment="JUSTIFY" textFill="#ffee33">
               <font>
                  <Font name="Calibri Bold" size="26.0" />
               </font>
               <effect>
                  <DropShadow blurType="TWO_PASS_BOX" color="#f2fc32" height="0.0" radius="19.515" width="80.06" />
               </effect>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
             <Button fx:id="Options" blendMode="SCREEN" mnemonicParsing="false" onAction="#Options" opacity="0.94" prefHeight="51.0" prefWidth="198.0" style="-fx-background-color: #0000CD;" text="Options" textAlignment="JUSTIFY" textFill="#ffee33">
