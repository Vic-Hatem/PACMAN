                  <Font name="Calibri Bold" size="26.0" />
               </font>
               <effect>
                  <DropShadow blurType="TWO_PASS_BOX" color="#f2fc32" height="0.0" radius="19.515" width="80.06" />
               </effect>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="rules" blendMode="SCREEN" mnemonicParsing="false" onAction="#Rules" opacity="0.94" prefHeight="51.0" prefWidth="198.0" style="-fx-background-color: #0000CD;" text="RULES" textAlignment="JUSTIFY" textFill="#ffee33">
               <font>
                  <Font name="Calibri Bold" size="26.0" />
               </font>
               <effect>
                  <DropShadow blurType="TWO_PASS_BOX" color="#f2fc32" height="0.0" radius="19.515" width="80.06" />
