         <BorderPane.margin>
            <Insets right="30.0" />
         </BorderPane.margin>
      </VBox>
   </right>
   <center>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="482.0" prefWidth="219.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="276.0" fitWidth="240.0" preserveRatio="true">
